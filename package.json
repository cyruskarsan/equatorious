{
  "name": "equatorious",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm build:css && next build",
    "start": "npm watch:css && next start",
    "lint": "next lint",
    "prettier": "prettier --write \"**/*.{js,json,css,md}\" --ignore-path \"./.gitignore\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@usedapp/core": "^0.5.6",
    "autoprefixer": "^10.4.0",
    "next": "12.0.3",
    "postcss-cli": "^9.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "npm run prettier --write --"
  }
}
